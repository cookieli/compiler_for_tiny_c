import printf;
bool test_short_circuit ( bool andy, bool okay ) {
  if ( okay != true ) {
    if ( andy != true ) {
      printf ( "short circuit failed in and\n" );
    }
    else {
      printf ( "short circuit failed in or\n" );
    }
  }
  return true;
}

void main(){
   bool a;
   a = true;
   if(a!= true && test_short_circuit(true, false)){
          printf("hello world\n");
   } else{
         printf("wrong\n");
   }
}